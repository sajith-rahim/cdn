{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "body": [
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": "auto",
                    "items": [
                        {
                            "type": "Image",
                            "url": "data:image/png;base64,",
                            "width": "56px",
                            "height": "56px",
                            "altText": "Logo"
                        }
                    ]
                },
                {
                    "type": "Column",
                    "width": "stretch",
                    "items": [
                        {
                            "type": "TextBlock",
                            "text": "UIB/UXF-R Office Hours",
                            "wrap": true,
                            "weight": "Bolder",
                            "style": "heading"
                        },
                        {
                            "type": "ColumnSet",
                            "targetWidth": "AtLeast:Standard",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "auto",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "3PM IST",
                                            "wrap": true
                                        }
                                    ],
                                    "spacing": "None",
                                    "verticalContentAlignment": "Center"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "TextBlock",
            "text": "   ",
            "wrap": true
        },
        {
            "type": "TextBlock",
            "text": "We have UIB Office hours at 3PM today. If you are in support, you are expected to join and help answer queries for our internal customers. Others please consider joining if you're available.",
            "wrap": true,
            "spacing": "None"
        },
        {
            "type": "ActionSet",
            "actions": [
                {
                    "type": "Action.OpenUrl",
                    "title": "Meeting Link",
                    "url": "https://servicenow.zoom.us/j/96798268728?from=addon",
                    "iconUrl": "https://mediacdn.pages.dev/zoom.png",
                    "style": "positive"
                },
                {
                    "type": "Action.OpenUrl",
                    "title": "Support Schedule",
                    "iconUrl": "https://mediacdn.pages.dev/power_point.png",
                    "tooltip": "Open Support Rotation Schedule",
                    "url": "https://servicenow.sharepoint.com/:x:/r/sites/UIBuilder/_layouts/15/Doc.aspx?sourcedoc=%7B9A2E4D15-FA47-4277-96EF-43E00E6E6161%7D&file=UIB%20Support%20Rotation.xlsx&action=default&mobileredirect=true"
                }
            ]
        }
    ]
}